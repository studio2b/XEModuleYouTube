<query id='updateCache' action='update'>
	<tables>
		<table name='youtube_videos' />
	</tables>
	<columns>
		<column name='title' var='title' />
		<column name='description' var='description' />
		<column name='channel' var='channel' />
		<column name='utc' var='utc' filter='number' />
		<column name='item' var='item' />
		<column name='timestamp' var='timestamp' />
	</columns>
	<conditions>
		<condition operation='equal' column='id' var='id' />
		<condition operation='equal' column='no' var='no' filter='number' pipe='and' />
	</conditions>
</query>